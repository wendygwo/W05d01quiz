<h1>Users list</h1>

<table>
	<thead>
		<tr>
			<th>User name</th>
			<th>Email</th>
			<th>Birthday</th>
		</tr>
	</thead>
	<tbody>
	
		<% @users.each do |user| %>
			<tr>
				<td><%=user.username %></td>
				<td><%=user.email %></td>
				<td><%=user.dob %></td>
				<td><%= link_to 'Edit this user', edit_user_path(user) %> </td>
			
			</tr>
		<% end %>
	</tbody>

</table>


<%= link_to 'Add new user', new_user_path %>